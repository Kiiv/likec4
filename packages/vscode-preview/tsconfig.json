{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@likec4/tsconfig/react-lib.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "src",
    "composite": true,
    "noPropertyAccessFromIndexSignature": false,
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "types": [
      "vite/client",
      "vscode-webview"
    ]
  },
  "include": [
    "src"
  ],
  "references": [
    {
      "path": "../core"
    },
    {
      "path": "../diagram"
    }
  ]
}
