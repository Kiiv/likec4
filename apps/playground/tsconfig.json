{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@likec4/tsconfig/react-lib.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "types": [
      "vite/client"
    ],
    "noEmit": true,
    "paths": {
      "#monaco/*": [
        "./src/monaco/*"
      ]
    },
    "noUnusedParameters": false
  },
  "include": [
    "./src"
  ],
  "references": [
    {
      "path": "../../packages/core"
    },
    {
      "path": "../../packages/diagram"
    },
    {
      "path": "../../packages/language-server"
    },
    {
      "path": "../../packages/layouts"
    },
    {
      "path": "../../packages/log"
    }
  ]
}
