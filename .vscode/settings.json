{
  "typescript.tsdk": "./node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "eslint.enable": false,
  "[typescript]": {
    "editor.defaultFormatter": "dprint.dprint",
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dprint.dprint",
  },
  "[javascript]": {
    "editor.defaultFormatter": "dprint.dprint",
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "dprint.dprint",
  },
  "[json]": {
    "editor.defaultFormatter": "dprint.dprint",
  },
  "[json5]": {
    "editor.defaultFormatter": "dprint.dprint",
  },
  "[markdown]": {
    "editor.defaultFormatter": "dprint.dprint",
  },
  "[mdx]": {
    "editor.defaultFormatter": "dprint.dprint",
  },
  "[css]": {
    "editor.defaultFormatter": "dprint.dprint",
  },
  "files.readonlyInclude": {
    "**/routeTree.gen.ts": true
  },
  "files.watcherExclude": {
    "**/routeTree.gen.ts": true
  },
  "search.exclude": {
    ".yarn/**": true,
    "yarn.lock": true,
    "node_modules/**": true,
    "packages/*/dist/**": true,
    "packages/*/bundle/**": true,
    "**/routeTree.gen.ts": true
  },
  "files.exclude": {
    ".DS_Store": true,
    ".devcontainer": true,
    ".editorconfig": true,
    ".env": true,
    ".fleet": true,
    ".git": true,
    ".github": true,
    ".gitignore": true,
    ".husky": true,
    ".idea": true,
    ".tool-versions": true,
    ".turbo": true,
    ".vscode": true,
    "CHANGELOG.md": true,
    "CONTRIBUTING.md": true,
    "LICENSE": true,
    "README.md": true,
    "apps": true,
    "dprint.json": true,
    "e2e": true,
    "examples": true,
    "node_modules": true,
    "package.json": true,
    "patches": true,
    "pnpm-lock.yaml": true,
    "pnpm-workspace.yaml": true,
    "ship.config.cjs": true,
    "styled-system": true,
    "tsconfig.json": true,
    "turbo.json": true,
    "vitest.workspace.ts": true,
    "apps/.DS_Store": true,
    "apps/docs": true,
    "apps/playground": true,
    "packages/.DS_Store": true,
    "packages/create-likec4": true,
    "packages/diagram": true,
    "packages/generators": true,
    "packages/icons": true,
    "packages/language-server": true,
    "packages/layouts": true,
    "packages/likec4": true,
    "packages/log": true,
    "packages/mcp": true,
    "packages/tsconfig": true,
    "packages/vscode": true,
    "packages/vscode-preview": true,
    "**/*.tsbuildinfo": true,
    "**/.turbo": true,
    "**/.wireit": true,
    "**/.wrangler": true,
    ".pnpm-store/**": true,
    "packages/likec4/__app__": true,
    "packages/*/.vscode-test-web": true,
    "packages/*/node_modules": true,
    "packages/*/styled-system": true,
    "packages/*/lib": true,
    "apps/*/node_modules": true,
    "apps/*/lib": true,
    "apps/*/styled-system": true,
    "styled-system/*/node_modules": true,
    "styled-system/*/lib": true,
    "e2e/node_modules/**": true
  },
  "json.schemas": [
    {
      "url": "./node_modules/langium-cli/langium-config-schema.json",
      "fileMatch": [
        "langium-config.json"
      ]
    },
    {
      "url": "./.vscode/dprint-schema.json",
      "fileMatch": [
        "dprint.json"
      ]
    }
  ],
  "cSpell.words": [
    "clsx",
    "codegen",
    "execa",
    "gridify",
    "hookz",
    "khroma",
    "langium",
    "likec4",
    "mantine",
    "nodrag",
    "nonexhaustive",
    "nopan",
    "outfile",
    "pannable",
    "rawmodel",
    "reactflow",
    "remeda",
    "styleprops",
    "tanstack",
    "tilg",
    "typecheck",
    "webcola",
    "xyflow",
    "zoomable"
  ],
  "sonarlint.connectedMode.project": {
    "connectionId": "likec4",
    "projectKey": "likec4_likec4"
  },
  "monorepoFocusWorkspace.internal": {
    "root": {
      "managedFilesIgnoreEntries": [
        ".DS_Store",
        ".devcontainer",
        ".editorconfig",
        ".env",
        ".fleet",
        ".git",
        ".github",
        ".gitignore",
        ".husky",
        ".idea",
        ".tool-versions",
        ".turbo",
        ".vscode",
        "CHANGELOG.md",
        "CONTRIBUTING.md",
        "LICENSE",
        "README.md",
        "apps",
        "dprint.json",
        "e2e",
        "examples",
        "node_modules",
        "package.json",
        "patches",
        "pnpm-lock.yaml",
        "pnpm-workspace.yaml",
        "ship.config.cjs",
        "styled-system",
        "tsconfig.json",
        "turbo.json",
        "vitest.workspace.ts",
        "apps/.DS_Store",
        "apps/docs",
        "apps/playground",
        "packages/.DS_Store",
        "packages/create-likec4",
        "packages/diagram",
        "packages/generators",
        "packages/icons",
        "packages/language-server",
        "packages/layouts",
        "packages/likec4",
        "packages/log",
        "packages/mcp",
        "packages/tsconfig",
        "packages/vscode",
        "packages/vscode-preview"
      ],
      "focusedWorkspace": "@likec4/core"
    }
  }
}
